setOpacity 0.5 $ setBrightness 2 $ setPosX (sin 0.1) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4";
setOpacity 0.5 $ setPosX (sin 0.01) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4";


