Agua, Laguna, Corriente 

setPosX 0.5 $ setPosY (0.0) $ video "http://yupanasimi.org/video/014.mp4";
setPosX (-0.5) $ setPosY (0.0) $ video "http://yupanasimi.org/video/014.mp4";
setPosX (sin 0.1) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4";
setPosX (sin 0.01) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4"

setPosX 0.5 $ setPosY (0.0) $ video "http://yupanasimi.org/video/014.mp4";
setBrightness 5 $ setPosX (-0.5) $ setPosY (0.0) $ video "http://yupanasimi.org/video/014.mp4";
setBrightness 2 $ setPosX (sin 0.1) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4";
setPosX (sin 0.01) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4"

setPosX 0.5 $ setPosY (0.0) $ video "http://yupanasimi.org/video/014.mp4";
setOpacity 0.5 $ setBrightness 5 $ setPosX (-0.5) $ setPosY (0.0) $ video "http://yupanasimi.org/video/014.mp4";
setBrightness 2 $ setPosX (sin 0.1) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4";
setOpacity 0.5 $ setPosX (sin 0.01) $ setSize 1 $ video "http://yupanasimi.org/video/014.mp4"



